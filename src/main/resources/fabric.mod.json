{
  "schemaVersion": 1,
  "id": "yunomakegoodmap",
  "version": "${version}",

  "name": "YUNoMakeGoodMap",
  "description": "Ever played a SkyBlock map, and had some cheeky person come along and reach the edge of the void and dry land? Well this mod fixes that, by generating the world itself completely void. You can select your starting structure and off you go. (Backported to beta 1.7.3 by Yunivers)",
  "authors": [
    "LexManos",
    "Yunivers"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/yunomakegoodmap",
    "sources": "https://github.com/AITYunivers/YUNoMakeGoodMap-b1.7.3",
    "issues": "https://github.com/AITYunivers/YUNoMakeGoodMap-b1.7.3/issues"
  },

  "license": "Apache 2.0",
  "icon": "assets/yunomakegoodmap/icon.png",

  "environment": "*",
  "mixins": [
    "yunomakegoodmap.mixins.json"
  ],
  "entrypoints": {
    "gcapi3": [
      "io.github.yunivers.yunomakegoodmap.config.Config"
    ],
    "gcapi3:factory_provider": [
      "io.github.yunivers.yunomakegoodmap.config.PlatformTypeEnumConfigFactory"
    ]
  },
  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
